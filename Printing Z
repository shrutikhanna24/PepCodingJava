import java.util.*;

public class Main {

  public static void main(String[] args) 
{
for(int i=0;i<5;i++)
{
    for(int j=0;j<5;j++)
    {
        if(i==0 || i==4)
        {
            System.out.print("*");
        }
        else
        {
            for(int k=i;k<=4-i;k++)
            {
                System.out.print(" ");
            }
            System.out.print("*");
            break;
        }
    }
     System.out.println();
}
  }
}
